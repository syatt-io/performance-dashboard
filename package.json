{
  "name": "performance-dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:frontend": "next dev -p 3001",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "worker": "tsx src/worker.ts",
    "scheduler": "tsx src/scheduler.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\" \"npm run worker\" \"npm run scheduler\"",
    "build": "prisma generate && tsc -p . --skipLibCheck && ls -la dist/src/ && next build",
    "build:frontend": "next build",
    "start": "NODE_ENV=production PORT=3000 concurrently \"node dist/src/index.js\" \"node dist/src/worker.js\" \"node dist/src/scheduler.js\"",
    "start:frontend": "next start -p 3001",
    "start:worker": "node dist/src/worker.js",
    "start:scheduler": "node dist/src/scheduler.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "setup:db": "node scripts/setup-db.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "bull": "^4.16.5",
    "bull-board": "^1.7.2",
    "chrome-launcher": "^1.2.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "google-auth-library": "^10.3.0",
    "ioredis": "^5.7.0",
    "lighthouse": "^12.8.2",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prisma": "^6.16.1",
    "puppeteer": "^24.22.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.0",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5"
  }
}
